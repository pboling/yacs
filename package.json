{
  "name": "dexcelerate-fe-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js",
    "server": "TEST_FAST=0 node server/index.js",
    "build": "node ./node_modules/typescript/bin/tsc -b && node ./node_modules/vite/bin/vite.js build",
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
    "lint:unix": "node ./node_modules/eslint/bin/eslint.js . --fix --format unix",
    "lint:fix": "node ./node_modules/eslint/bin/eslint.js . --fix",
    "lint:ci": "node ./node_modules/eslint/bin/eslint.js . --config eslint.ci.config.js --max-warnings=0",
    "lint:ci:fix": "node ./node_modules/eslint/bin/eslint.js . --config eslint.ci.config.js --fix --max-warnings=0",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "dev:serve": "concurrently -k -s first -n server,frontend -c blue,green \"npm run server\" \"npm run dev\"",
    "dev:all": "npm run test:all && npm run dev:serve",
    "test": "node --test",
    "test:e2e": "TEST_FAST=1 playwright test",
    "test:e2e:ui": "TEST_FAST=1 playwright test --ui",
    "test:all": "npm run lint && node --test",
    "generate-symbols": "node scripts/generate-symbols-from-scowl.mjs",
    "format:prettier": "prettier . --write",
    "format:check": "prettier . --check",
    "format": "npm run format:prettier && npm run lint:fix"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "express": "^4.19.2",
    "lucide-react": "^0.468.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.48.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-react-dom": "*",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "*",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "ws": "^8.18.0",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}

{
  "name": "dexcelerate-fe-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js",
    "server": "node server/index.js",
    "build": "node ./node_modules/typescript/bin/tsc -b && node ./node_modules/vite/bin/vite.js build",
    "lint": "node ./node_modules/eslint/bin/eslint.js .",
    "preview": "node ./node_modules/vite/bin/vite.js preview",
    "dev:serve": "concurrently -k -s first -n server,frontend -c blue,green \"npm run server\" \"npm run dev\"",
    "dev:all": "npm run lint && node --test && npm run dev:serve",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "express": "^4.19.2",
    "concurrently": "^9.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-dom": "*",
    "eslint-plugin-react-x": "*",
    "globals": "^16.4.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "ws": "^8.18.0",
    "@playwright/test": "^1.48.2"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}

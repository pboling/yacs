{
  "name": "dexcelerate-fe-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "vite",
    "dev:all": "pnpm run format && pnpm run test && vite",
    "dev:serve": "concurrently -k -s first -n server,frontend -c blue,green \"pnpm run server\" \"pnpm run dev\"",
    "dev:serve:local": "export VITE_API_BASE=\"http://localhost:3001\"\nexport VITE_WS_URL=\"ws://localhost:3001/ws\"\nexport VITE_DISABLE_WS=0 && export TEST_FAST=1 && pnpm dev:serve",
    "fixture:scanner": "node scripts/fetch-scanner-fixture.mjs",
    "fixture:sets": "node scripts/fetch-trending-and-new-fixtures.mjs",
    "format:prettier": "prettier . --write",
    "format:check": "prettier . --check",
    "format": "pnpm run format:prettier && pnpm run lint:fix",
    "generate-symbols": "node scripts/generate-symbols-from-scowl.mjs",
    "lint": "eslint .",
    "lint:ci": "eslint . --config eslint.ci.config.js --max-warnings=0",
    "lint:ci:fix": "eslint . --config eslint.ci.config.js --fix --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "lint:unix": "eslint . --fix --format unix",
    "preview": "vite preview",
    "server": "TEST_FAST=0 node server/index.js",
    "test:unit": "vitest run",
    "test:unit:json": "vitest run --reporter=json --outputFile vitest-results.json",
    "test": "pnpm run test:unit",
    "test:e2e": "TEST_FAST=1 playwright test",
    "test:e2e:ui": "TEST_FAST=1 playwright test --ui",
    "test:e2e:ci": "node scripts/run-e2e-ci.mjs",
    "test:all": "pnpm run test && pnpm run test:e2e"
  },
  "dependencies": {
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@tanstack/react-virtual": "^3.13.12",
    "lucide-react": "^0.468.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@playwright/test": "^1.55.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-dom": "^2.0.6",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-react-x": "^2.0.6",
    "express": "^4.21.2",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.45.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitest": "^3.2.4",
    "ws": "^8.18.3"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}

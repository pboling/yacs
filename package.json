{
  "name": "dexcelerate-fe-test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "TEST_FAST=0 node server/index.js",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:unix": "eslint . --fix --format unix",
    "lint:fix": "eslint . --fix",
    "lint:ci": "eslint . --config eslint.ci.config.js --max-warnings=0",
    "lint:ci:fix": "eslint . --config eslint.ci.config.js --fix --max-warnings=0",
    "preview": "vite preview",
    "dev:serve": "concurrently -k -s first -n server,frontend -c blue,green \"pnpm run server\" \"pnpm run dev\"",
    "dev:all": "pnpm run test:all && pnpm run dev:serve",
    "test:unit": "vitest run",
    "test": "vitest run && node --test",
    "test:e2e": "TEST_FAST=1 playwright test",
    "test:e2e:ui": "TEST_FAST=1 playwright test --ui",
    "test:all": "pnpm run format && pnpm run test && pnpm run test:e2e",
    "generate-symbols": "node scripts/generate-symbols-from-scowl.mjs",
    "format:prettier": "prettier . --write",
    "format:check": "prettier . --check",
    "format": "pnpm run format:prettier && pnpm run lint:fix",
    "fixture:scanner": "node scripts/fetch-scanner-fixture.mjs"
  },
  "dependencies": {
    "concurrently": "^9.1.2",
    "express": "^4.19.2",
    "lucide-react": "^0.468.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@playwright/test": "^1.48.2",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-formatter-unix": "^8.40.0",
    "eslint-plugin-react-dom": "*",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-react-x": "*",
    "globals": "^16.4.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitest": "^2.1.3",
    "ws": "^8.18.0"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.1.12"
  }
}
